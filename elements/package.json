{
  "name": "@zoompinch/elements",
  "description": "Custom elements wrapper ZoomPinch - reactive pinch & zoom component",
  "version": "0.0.25",
  "private": false,
  "type": "module",
  "main": "./dist/zoompinch-elements.umd.js",
  "module": "./dist/zoompinch-elements.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/zoompinch-elements.es.js",
      "require": "./dist/zoompinch-elements.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "custom elements",
    "zoompinch",
    "zoom images",
    "pinch to zoom",
    "image zoom",
    "pan gesture",
    "pan and zoom",
    "panzoom",
    "gesture library",
    "touch gestures"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vite": "^6.4.1"
  },
  "dependencies": {
    "@zoompinch/core": "^0.0.25"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://zoompinch.pages.dev/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ElyaConrad/zoompinch"
  }
}
