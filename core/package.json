{
  "name": "@zoompinch/core",
  "description": "Pinch-and-zoom experience that's feels native and communicates the transform reactively and lets you project any layer on top of the transformed canvas",
  "version": "0.0.8",
  "private": false,
  "type": "module",
  "main": "./dist/zoompinch-core.umd.js",
  "module": "./dist/zoompinch-core.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/zoompinch-core.es.js",
      "require": "./dist/zoompinch-core.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "dev": "vite"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vite": "^6.4.1"
  }
}