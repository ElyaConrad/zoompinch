{
  "name": "@zoompinch/vue",
  "description": "Vue wrapper for ZoomPinch - reactive pinch & zoom component",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/zoompinch-vue.umd.js",
  "module": "./dist/zoompinch-vue.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/zoompinch-vue.es.js",
      "require": "./dist/zoompinch-vue.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.4",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vite-plugin-dts": "^4.5.4",
    "vue-tsc": "^3.2.0"
  },
  "dependencies": {
    "@zoompinch/core": "workspace:^"
  },
  "peerDependencies": {
    "vue": "^3.5.26"
  },
  "publishConfig": {
    "access": "public"
  }
}
