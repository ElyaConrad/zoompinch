{
  "name": "zoompinch",
  "private": false,
  "version": "0.0.19",
  "type": "module",
  "files": [
    "package.json",
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/zoompinch.es.js",
      "require": "./lib/zoompinch.umd.js"
    },
    "./style.css": "./lib/style.css"
  },
  "types": "./lib/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "npm link && npm-watch build",
    "preview": "vite preview"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,vue,html,scss",
      "quiet": false
    }
  },
  "dependencies": {
    "@vueuse/core": "^10.9.0"
  },
  "peerDependencies": {
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@vitejs/plugin-vue": "^3.1.0",
    "npm-watch": "^0.11.0",
    "sass": "^1.56.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^1.6.6",
    "vue-tsc": "^2.0.7"
  }
}
